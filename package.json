{
  "name": "@luban-ui/vite-plugin-vue",
  "version": "0.0.1",
  "main": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist",
    "types",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "husky install",
    "build-lib": "vite build --config scripts/config/vite.config.lib.ts",
    "build-es": "vite build --config scripts/config/vite.config.es.ts",
    "build-dts": "rm -rf _types && rm -rf types && vue-tsc --declaration --emitDeclarationOnly --project tsconfig.build.json && mv _types/src dist/types && rm -rf _types",
    "build": "rm -rf dist && npm run build-lib && npm run build-es && npm run build-dts",
    "style:dts": "ts-node-esm --experimentalSpecifierResolution node scripts/cssModulesDTS.ts",
    "pub": "npm publish --access=public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "2.0.0",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "glob": "^10.3.10",
    "minimatch": "^9.0.3",
    "node-watch": "^0.7.4",
    "rollup-plugin-visualizer": "^5.11.0",
    "sass": "^1.69.5",
    "terser": "^5.26.0",
    "typed-css-modules": "^0.8.1",
    "vite-plugin-circullar-dependency": "^0.0.2",
    "vite-plugin-legacy-extends": "^5.2.0",
    "vite-svg-loader": "^5.1.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "postcss-scss": "^4.0.9",
    "prettier": "~3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.2",
    "vite": "^5.0.5",
    "vue-tsc": "^1.8.25"
  },
  "repository": "https://github.com/ccqgithub/lu-ban",
  "homepage": "https://github.com/ccqgithub/lu-ban"
}
