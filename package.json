    "@luban-ui/eslint-config": "^1.0.9",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "~5.3.2",
    "vite": "^5.0.10"
